utlined width=\"100px\" height=\"100px\" class=\"d-flex headline\"\n              >{{ AQIdata.SiteName }}\n            </v-btn>\n          </div>\n          <v-divider class=\"mt-5\"></v-divider>\n          <div class=\"mt-5\">\n            <p class=\"headline\">O3 臭氧</p>\n            <v-slider\n              class=\"v-slider\"\n              v-model=\"AQIdata.O3\"\n              thumb-label=\"always\"\n              :thumb-color=\"AQIinfo.color\"\n              :color=\"AQIinfo.color\"\n              readonly\n              :persistent-hint=\"AQIinfo.msg\"\n              thumb-size=\"32\"\n              height=\"1\"\n              bar-height=\"40\"\n            ></v-slider>\n          </div>\n          <div class=\"mt-5\">\n            <p class=\"headline\">PM2.5 細懸浮微粒 : {{ AQIdata.PM25 }}</p>\n            <v-progress-linear\n              :value=\"AQIdata.PM25\"\n              height=\"15\"\n              Opacity=\"0.3\"\n              color=\"amber darken-3\"\n              rounded\n            >\n            </v-progress-linear>\n          </div>\n          <div class=\"mt-5\">\n            <p class=\"headline\">CO 一氧化碳 : {{ AQIdata.CO }}</p>\n            <v-progress-linear\n              :value=\"AQIdata.CO * 100\"\n              height=\"15\"\n              Opacity=\"0.3\"\n              color=\"amber darken-3\"\n              rounded\n            >\n            </v-progress-linear>\n          </div>\n        </v-card>\n      </v-col>\n    </v-row>\n  </v-content>\n</template>\n\n<script>\nexport default {\n  props: [\"AQIdata\"],\n  data: () => {\n    return {\n      AQIinfo: {\n        color: \"green accent-3\",\n        msg: \"良好\"\n      }\n    };\n  },\n  methods: {\n    setO3color(val) {\n      if (parseInt(val) <= 50) {\n        this.AQIinfo.color = \"green accent-3\";\n        this.AQIinfo.msg = \"良好\";\n      } else if (parseInt(val) <= 100) {\n        this.AQIinfo.color = \"yellow lighten-2\";\n        this.AQIinfo.msg = \"普通\";\n      } else if (parseInt(val) <= 150) {\n        this.AQIinfo.color = \"orange darken-2\";\n        this.AQIinfo.msg = \"對敏感族群不健康\";\n      } else if (parseInt(val) <= 200) {\n        this.AQIinfo.color = \"red \";\n        this.AQIinfo.msg = \"對所有族群不健康\";\n      } else if (parseInt(val) <= 300) {\n        this.AQIinfo.color = \"purple\";\n        this.AQIinfo.msg = \"非常不健康\";\n      } else {\n        this.AQIinfo.color = \"brown\";\n        this.AQIinfo.msg = \"不要出門\";\n      }\n    }\n  }\n};\n</script>\n<style scoped>\n.v-slider {\n  height: 25px !important;\n  font-size: 2rem;\n}\n</style>\n"]}]}